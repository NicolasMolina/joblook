@charset "utf-8";
/* CSS Document */

ol,ul {
	list-style:none;
	margin:0px;
	padding:0px;
}

a,img a,img,a img {
	border:0;
	outline:0;
	text-decoration:none;
}

img {vertical-align:top; max-width:100%;}

h1,
h2,
h3,
h4,
h5,
h6,
p {
  margin:0px;
  font-family: inherit;
  font-weight: normal;
  color: inherit;
}

/* Float Elements 
---------------------------------*/
.fl-lt {float:left;}
.fl-rt {float:right;}

/* Clear Floated Elements
---------------------------------*/
.clear {
  clear: both;
  display: block;
  overflow: hidden;
  visibility: hidden;
  width: 0;
  height: 0;
}

.clear2 {
  clear: both;
  display: block;
  overflow: hidden;
  visibility: hidden;
  width: 0;
  height: 0;
  padding:0 0 15px 0;
}

.clearfix:before,
.clearfix:after {
  content: '\0020';
  display: block;
  overflow: hidden;
  visibility: hidden;
  width: 0;
  height: 0;
}

.clearfix:after {
  clear: both;
}

